    function getnote(UserNoteMLSID){
        var UserID=@PkeyUserId;
        if(UserID=="" || UserID.length<0)
        {
            $('#span_mynote').html("");
            $('#navLogin').click();
            $('#ModelDetails').modal("hide");
            $('#MyNotePopup').modal("hide");
            $('#PhoneCallModel').modal("hide");
        }
        else
        {
            $.ajax({
                url: "/Customer/SearchFilter/GetUserNoteData",
                type: "POST",
                dataType: "json",
                data: { MLSID: UserNoteMLSID },
                success: function (data) {
                    $('#MyNote').val('');
