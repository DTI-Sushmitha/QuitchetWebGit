    //Function TO Assign Values For Home Filters
    function HomeFilterValuesAssigning()
    {
        filtertype = "1";
        hmasterfeatures = [];
        $.each($("input[name='masterfeatures']:checked"), function () {
            hmasterfeatures.push($(this).val());
        });
        hmasterfeatures = hmasterfeatures.join(",");
