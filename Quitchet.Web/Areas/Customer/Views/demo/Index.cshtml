    //------------------------Dynamic Seek bobbule Functionality---------------------------
    function DynamicSeekBobble(SPoint,EPoint,SliderWidth,min,max,rh,lh){
        var newPoint, offset, newPlace;
        var newPoint1, offset1, newPlace1,width;
        var minimum = min;
        var maximum = max;
        var width=SliderWidth.split('px')[0];


        // Figure out placement percentage between left and right of input
        if(minimum==0 && maximum==100)
        {
            newPoint = (getDupeValue(SPoint) - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
            newPoint1 = (getDupeValue(EPoint) - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
        }
        else
        {
            newPoint = (SPoint - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
            newPoint1 = (EPoint - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
        }
        // Janky value to get pointer to line up better
        offset = 0;
        offset1 = 0;
        // Prevent bubble from going beyond left or right (unsupported browsers)
        if (newPoint < 0) { newPlace = 0; }
        else if (newPoint > 1) { newPlace = width; }
        else { newPlace = width * newPoint + offset; offset -= newPoint; }
        if (newPoint1 < 0) { newPlace1 = 0; }
        else if (newPoint1 > 1) { newPlace1 = width; }
        else { newPlace1 = width * newPoint1 + offset1; offset1 -= newPoint1; }
        $('#'+rh).css('left', newPlace);
        $('#'+rh).css('marginleft', offset + "%");
        $('#'+lh).css('left', newPlace1);
        $('#'+lh).css('marginleft', offset1 + "%");

        var statd=SPoint;
        var endd=EPoint;



        if(min==0 && max==100)
        {

            if (statd.toString().length == 5) {
                statd = '$' + statd.toString().substring(0, 2) + 'K';
            }
            if (statd.toString().length == 6) {
                statd = '$' + statd.toString().substring(0, 3) + 'K';
            }
            if (statd.toString().length == 7) {
                var milion = statd.toString().substring(0, 2);
                var splitmilion = milion.toString().substring(0, 1) + '.' + milion.toString().substring(1, 2);
                statd = '$' + splitmilion + 'M';
            }

            if (endd.toString().length == 5) {
                endd = '$' + endd.toString().substring(0, 2) + 'K';
            }
            if (endd.toString().length == 6) {
                endd = '$' + endd.toString().substring(0, 3) + 'K';
            }

            if (endd.toString().length == 7) {
                var milion = endd.toString().substring(0, 2);
                var splitmilion = milion.toString().substring(0, 1) + '.' + milion.toString().substring(1, 2);
                endd = '$' + splitmilion + 'M';
            }

            if(SPoint==min && EPoint==max)
            {
                $('#'+rh).html('No Min');
                $('#'+lh).html('No Max');
            }
            else
            {
                $('#'+rh).html(statd);
                $('#'+lh).html(endd);
            }
        }

        if(min==0 && max==80001)
        {
            if (statd.toString().length == 3) {
                statd = '$' + statd.toString() + '/mo';
            }
            if (statd.toString().length == 4) {
                statd = '$' + statd/100 + 'K/mo';
            }
            if (statd.toString().length == 5) {
                statd = '$' + statd.toString().substring(0, 3) + 'K/mo';
            }
            if (statd.toString().length == 6) {
                var milion = statd.toString().substring(0, 2);
                var splitmilion = milion.toString().substring(0, 1) + '.' + milion.toString().substring(1, 2);
                statd = '$' + splitmilion + 'M/mo';
            }
            if (endd.toString().length == 3) {
                endd = '$' + endd.toString().substring(0, 2) + 'K/mo';
            }
            if (endd.toString().length == 4) {
                endd = '$' + endd.toString().substring(0, 2) + 'K/mo';
            }
            if (endd.toString().length == 5) {
                endd = '$' + endd.toString().substring(0, 3) + 'K/mo';
            }
            if (endd.toString().length == 6) {
                var milion = endd.toString().substring(0, 2);
                var splitmilion = milion.toString().substring(0, 1) + '.' + milion.toString().substring(1, 2);
                endd = '$' + splitmilion + 'M/mo';
            }
            //endd = '$' + endd.toString() + '/mo';
            if(SPoint==min && EPoint==max)
            {
                $('#'+rh).html('No Min');
                $('#'+lh).html('No Max');
            }
            else
            {
                $('#'+rh).html(statd);
                $('#'+lh).html(endd);
            }

        }

        if(min==0 && max==7000)
        {

            if(SPoint==0 && EPoint==7000){
                $('#'+rh).html('No Min');
                $('#'+lh).html('No Max');
            }
            else if(SPoint!=0 && EPoint==7000){
                $('#'+rh).html(statd + ' Sqft');
                $('#'+lh).html('No Max');
            }
            else if(SPoint==0 && EPoint!=7000){
                $('#'+rh).html('No Min');
                $('#'+lh).html(endd + ' Sqft');
            }
            else{
                $('#'+rh).html(statd + ' Sqft');
                $('#'+lh).html(endd + ' Sqft');
            }
        }
        if(min==0 && max==50)
        {

            if(SPoint==0 && EPoint==50){
                $('#'+rh).html('No Min');
                $('#'+lh).html('No Max');
            }
            else if(SPoint!=0 && EPoint==50){
                $('#'+rh).html(statd +' acres');
                $('#'+lh).html('No Max');
            }
            else if(SPoint==0 && EPoint!=50){
                $('#'+rh).html('No Min');
                $('#'+lh).html(endd+' acres');
            }
            else{
                $('#'+rh).html(statd+' acres');
                $('#'+lh).html(endd+' acres');
            }
        }

    }
