        var UserID=@PkeyUserId;
        if(UserID=="" || UserID.length<0)
        {
            $('#navLogin').click();
        }
        else
        {
            checklist = [];
            unchecklist = [];
            orderlist = [];
            $.each($("input[name='itemclick']:checked"), function (i, m) {
                checklist.push($(m).attr("id"));
            });
            $.each($("input[name='itemclick']:not(:checked)"), function (i, n) {
                unchecklist.push($(n).attr("id"));
            });
            $.each($("input[name='itemclick']"), function (i, o) {
                orderlist.push($(o).val());
            });
            var obj = {};
            obj.MAINVIEW = false;
            obj.MASTERBEDROOM = false;
            obj.MASTERBATHROOM = false;
            obj.KITCHEN = false;
            obj.Living_Great_Room = false;
            obj.BACKVIEW = false;
            obj.Other_Interior = false;
            obj.OTHER = false;
            obj.ORDER1 = "";
            obj.ORDER2 = "";
            obj.ORDER3 = "";
            obj.ORDER4 = "";
            obj.ORDER5 = "";
            obj.ORDER6 = "";
            obj.ORDER7 = "";
            obj.ORDER8 = "";
            for (var j = 0; j <= checklist.length; j++) {

                if (checklist[j] == "checkboxMainView") {
                    obj.MAINVIEW = true;
                }
                else if (checklist[j] == "checkboxMasterBedroom") {
                    obj.MASTERBEDROOM = true;
                }
                else if (checklist[j] == "checkboxMasterBathroom") {
                    obj.MASTERBATHROOM = true;
                }
                else if (checklist[j] == "checkboxKitchen") {
                    obj.KITCHEN = true;
                }
                else if (checklist[j] == "checkboxLivingRoom") {
                    obj.Living_Great_Room = true;
                }
                else if (checklist[j] == "checkboxBackView") {
                    obj.BACKVIEW = true;
                }
                else if (checklist[j] == "checkboxOtherInterior") {
                    obj.Other_Interior = true;

                }
                else if (checklist[j] == "checkboxOtherView") {
                    obj.OTHER = true;
                }
            }

            for (var l = 0; l <= orderlist.length; l++) {
                if (l == 0) {
                    obj.ORDER1 = orderlist[l];
                }
                else if (l == 1) {
                    obj.ORDER2 = orderlist[l];
                }
                else if (l == 2) {
                    obj.ORDER3 = orderlist[l];
                }
                else if (l == 3) {
                    obj.ORDER4 = orderlist[l];
                }
                else if (l == 4) {
                    obj.ORDER5 = orderlist[l];
                }
                else if (l == 5) {
                    obj.ORDER6 = orderlist[l];
                }
                else if (l == 6) {
                    obj.ORDER7 = orderlist[l];
                }
                else if (l == 7) {
                    obj.ORDER8 = orderlist[l];
                }
            }
