        $('#phonediv1').hide();
        $('#phonediv2').hide();
        $('#emaildiv1').hide();
        $('#emaildiv2').hide();
        $('#addrdiv1').hide();
        $('#addrdiv2').hide();
        $('#deletepopup').hide();
        $('#sharepopup').hide();
        $(".nav-tabs a").click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

    });
    $("#addcontacts,#editcontact").click(function (e) {
        $("#popup2").show();
    });

    $("#add1").click(function () {
        $('#phonediv1').show();
    });
    $("#add2").click(function () {
        $('#phonediv2').show();
        $('#add3').hide();

    });
    $("#del1").click(function () {
        $('#phonediv1').hide();
    });
    $("#del2").click(function () {
        $('#phonediv2').hide();
    });
    $("#addemail1").click(function () {
        $('#emaildiv1').show();
    });
    $("#addemail2").click(function () {
        $('#emaildiv2').show();
        $('#addemail3').hide();

    });
    $("#delemail1").click(function () {
        $('#emaildiv1').hide();
    });
    $("#delemail2").click(function () {
        $('#emaildiv2').hide();
    });
    $("#addaddress1").click(function () {
        $('#addrdiv1').show();
    });
    $("#addaddress2").click(function () {
        $('#addrdiv2').show();

    });
    $("#deladdress1").click(function () {
        $('#addrdiv1').hide();
    });
    $("#deladdress2").click(function () {
        $('#addrdiv2').hide();
    });
    $("#cancelBtn").click(function () {
        $('#popup2').hide();
    });
    $("#deleteBtn").click(function () {
        $('#deletepopup').show();
        $('#popup2').hide();
    });
    $("#noBtn").click(function () {
        $('#popup2').show();
        $('#deletepopup').hide();
    });
    $("#sharecontact").click(function () {
        $('#sharepopup').show();
    });
    $("#cancelshareBtn").click(function () {
        $('#sharepopup').hide();
    });

