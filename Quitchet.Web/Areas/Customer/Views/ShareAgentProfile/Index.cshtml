                    $('#div_arearsserved').html(bindareas);
                    //$('#item1o').addClass('hide');
                    //$('#item2o').addClass('hide');
                    //$('#item3o').addClass('hide');
                    $('#item4o').hide();
                    //$('#item5o').addClass('hide');
                    //$('#item1').removeClass('item').addClass('hide');
                    //$('#item2').removeClass('item').addClass('hide');
                    //$('#item3').removeClass('item').addClass('hide');
                    $('#item4').removeClass('item').addClass('hide');
                    //$('#item5').removeClass('item').addClass('hide');
                    //$('#item2').hide();
                    //$('#item3').hide();
                    //$('#item4').hide();
                    //$('#item5').hide();
                    debugger;
                    if(data[0].Testimonials1 != null){
                     //   $('#item1o').removeClass('hide');
                        $('#item1p').text(data[0].Testimonials1);
                      //  $('#item1').removeClass('hide').addClass('item');

