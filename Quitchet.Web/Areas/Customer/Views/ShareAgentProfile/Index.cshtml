    function ActiveListingsofAgent()
    {
        $.ajax({
            url: "/Customer/ManageSearchAgent/ActiveListingsofAgent",
            type: "GET",
            dataType: "json",
            data: { agentid: viewagentid },
            success: function (data) {
                if (data.length > 0) {
                    bindActiveListings(data);
                }
            },
        });
    }
    function bindActiveListings(data)
    {
        $("#activeListingsDiv").empty();
        $.each(data, function (i, item) {
            var photo = "",price="",imagepin="",favimage="",sqftAcres="";
            if (item.Photolocation.toString().indexOf(',') > -1) {
                photo = item.Photolocation.split(',')[0];
            }
            else
                photo = item.Photolocation;
            if (item.Class == "Rental") {
                price = "$ " + (item.ListingPrice + "").replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,") + "/mo";
                if (item.Is_Viewd == "YES")
                    imagepin = '<img src="/img/viewed_graypin.png" height="20" style="position:relative; top:-2px" />'
                else
                    imagepin = '<img src="/img/gray_legendMenu.png" height="20" style="position:relative; top:-2px" />'

            }
            else {
                price = "$ " + (item.ListingPrice + "").replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,");
                if (item.Sale_Rent == "Sold") {
                    if (item.Is_Viewd == "YES")
                        imagepin = '<img src="/img/viewed_redpin.png" height="20" style="position:relative; top:-2px" />'
                    else
                        imagepin = '<img src="/img/red_legendMenu.png" height="20" style="position:relative; top:-2px" />'
                }
                else if (item.Sale_Rent == "Active") {
                    if (item.Is_Viewd == "YES")
                        imagepin = '<img src="/img/viewed_greenpin.png" height="20" style="position:relative; top:-2px" />'
                    else
                        imagepin = '<img src="/img/Green_legend_Menu.png" height="20" style="position:relative; top:-2px" />'
                }
                else if (item.Sale_Rent == "Pending") {
                    if (item.Is_Viewd == "YES")
                        imagepin = '<img src="/img/yellow_legendMenu.png" height="20" style="position:relative; top:-2px" />'
                    else
                        imagepin = '<img src="/img/viewed_yellowpin.png" height="20" style="position:relative; top:-2px" />'
                }
            }
            if (item.Is_Favorite == "YES")
            {
                favimage = '<a href="#" style="text-decoration:none"><img src="/img/hearticon.png" height="20" id="'+item.MLSId+'" style="position:relative; margin-top:25px" /></a>'
            }
            else {
                favimage = '<a href="#" style="text-decoration:none"><img src="/img/heartmi.png" height="20" id="'+item.MLSId+'" style="position:relative; margin-top:25px" /></a>'
            }
            if(item.Class=="Lots/Land"){
                sqftAcres=item.ApproxAcres + ' acres';
            }
            else{
                sqftAcres=item.ApproxAcres + ' sqft';
            }
            var listingdata = '<div class="col-sm-12" style="padding:0;height:101px;position:relative;border-bottom: 1px solid #ff5d26;">'
                + '<div class="col-sm-3" style="background-image:url(' + photo + ');background-repeat:no-repeat;background-size:100% 97%; height:100px;"></div>'
                +'<div class="col-sm-3" style="border-right:1px solid lightgray;padding-left:20px;height:100px;">'
                 +'<table><tr><td>&nbsp;</td></tr><tr><td>'+imagepin+''
                 +'<span style="font-weight:bold;color: #FF6937 !important;font-size:15px">'+price+'</span></td></tr>'
                 + '<tr><td><span style="font-weight:bold;font-size:16px">' + item.ADDRESKKS.split(',')[0] + '</span></td></tr>'
