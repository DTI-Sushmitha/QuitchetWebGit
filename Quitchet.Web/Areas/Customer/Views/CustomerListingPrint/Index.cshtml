    function AGL_HomePopupSlideImage(ClickStatus) {
        if (ClickStatus == 'Next') {
            ImageCount = parseInt(ImageCount + 1);
            $('#AGL_HomePopupImgDiv').css('background-image', 'url(' + Arr_Images[ImageCount - 1] + ')');
            $('#AGL_HomepopupPrevImgBtn').show();
            if (parseInt(ImageCount) == parseInt(PhotosCount))
                $('#AGL_HomepopupNextImgBtn').hide();
        }
        if (ClickStatus == 'Previous') {
            ImageCount = ImageCount - 1;
            if (parseInt(ImageCount) == 1)
                $('#AGL_HomepopupPrevImgBtn').hide();
            $('#AGL_HomepopupNextImgBtn').show();
            $('#AGL_HomePopupImgDiv').css('background-image', 'url(' + Arr_Images[ImageCount - 1] + ')');
        }
        $('#AFL_ImageCountOOF').html(ImageCount);
    }
