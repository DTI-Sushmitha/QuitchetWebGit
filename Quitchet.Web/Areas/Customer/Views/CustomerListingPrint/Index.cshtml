<script>
    var MlsID = ''; var AgentID = '';
    var AGL_SharePopURL = ""; var HomeType = ''; var Arr_Images = []; var ImageCount = 1; var PhotosCount = 0; var Str_Geolat = ''; var Str_Geolang = ''; var Arr_Showings = []; var Arr_DocOnOffer = ''; var Arr_MultiUploadImages = [];
    var Arr_lakeProperties = []; var Arr_Equestrian = []; var Arr_RoadFrontage = []; var Arr_Topography = []; var Arr_Aminities = []; var Arr_PropDescriptions = [];
    $(document).ready(function () {
        //-- Read Primarykey and userid from URl
        var Url = new URLSearchParams(window.location.search);
        MlsID = Url.get("Mlsid");
        UserID = Url.get("UserID");
        GetPropertydetailsByMls(MlsID, UserID);
    });
    function GetPropertydetailsByMls(MlsID, UserID) {
        $.ajax({
            url: "/FullDetails/GetCustomerListingDetails/",
            type: "GET",
            dataType: "json",
            data: { MlsID: MlsID, UserID: UserID },
            success: function (data) {
                BindDatatoHtml(data);
            },
            error: function () {
                alert('nodata');
            }
        });
    }
    function BindDatatoHtml(data) {
        $('#div_HomeMainFeatures,#div_HMFeaturesAndEminities,#div_HMutilitiesAndCooling,#div_HomeFinanceInfo').hide();
        $('#div_LotslandPropertyHomeFeatures,#utilities_Heating_Cooling,#div_LotsFinanceInfo,#lot_convenantsAndRestrictions').hide();
        if (data[0].Is_Favorite == "NO")
            $("#AGL_ImageFavourite").attr("src", "/img/heartmi.png");
        if (data[0].Is_Favorite == "YES")
            $("#AGL_ImageFavourite").attr("src", "/img/hearticon.png");
        //if (data[0].Is_Viewd == "NO")
        //    $('#AGL_ViewedIcon').hide();
        //else
        //    $('#AGL_ViewedIcon').show();
        if (data[0].Class == "Residential" || data[0].Class == "Residential") {
            if (data[0].Class == "Residential") {
                $('#Agent_HouseType').html("Home Details");
                $('#AGL_sale_rent').html('For Sale');
            }
            else if (data[0].Class == "Rental") {
                $('#Agent_HouseType').html("Rental Details");
                $('#AGL_sale_rent').html('For Rent');
            }
            $('#div_HomeMainFeatures,#div_HMFeaturesAndEminities,#div_HMutilitiesAndCooling,#div_HomeFinanceInfo').show();
            $('#div_LotslandPropertyHomeFeatures,#utilities_Heating_Cooling,#div_LotsFinanceInfo,#lot_convenantsAndRestrictions').hide();
        }
        else if (data[0].Class == "Lots/Land") {
            $('#Agent_HouseType').html("Lots / Land Detail");
            $('#AGL_sale_rent').html('For Sale');
            $('#div_HomeMainFeatures,#div_HMFeaturesAndEminities,#div_HMutilitiesAndCooling,#div_HomeFinanceInfo').hide();
            $('#div_LotslandPropertyHomeFeatures,#utilities_Heating_Cooling,#div_LotsFinanceInfo,#lot_convenantsAndRestrictions').show();
        }
        if (data[0].Status == "Active") {
            $('.RoundEdgespn').css('background-color', 'green');
            $('#AGL_sale_rent').html('For Sale');
        }
        else if (data[0].Status == "Sold") {
            $('.RoundEdgespn').css('background-color', 'red');
            $('#AGL_sale_rent').html('Sold');
        }
        else if (data[0].Status == "Pending" || data[0].Status == "Contingency Contract") {
            $('.RoundEdgespn').css('background-color', '#f0e243');
            $('#AGL_sale_rent').html('Under Contract');
        }
        else if (data[0].Status == "OpenHouse") {
            $('.RoundEdgespn').css('background-color', 'blue');
            $('#AGL_sale_rent').html('Open House');
        }
        if (data[0].Class == "Rental") {
            $('.RoundEdgespn').css('background-color', 'gray');
            $('#AGL_sale_rent').html('Rental');
        }
        var PropertyPrice = "$ " + (data[0].ListPrice + "").replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,");
        $('#lbl_price').html(PropertyPrice);
        if (data[0].Have_Agent == "YES") {
            if (data[0].UserAgent_Image != "" && data[0].UserAgent_Image != "," && data[0].UserAgent_Image != null) {
                $('#AGL_AgentImage').attr('src', data[0].UserAgent_Image);
            }
            else {
                $('#AGL_AgentImage').attr("src", "/img/addphotopic.png");
            }
            $('#AGLAgentName').html(data[0].UserAgent_FirstName + ' ' + data[0].UserAgent_LastName);
            $('#AGL_AgentAddressOne').html(data[0].UserAgent_OfficeName);
