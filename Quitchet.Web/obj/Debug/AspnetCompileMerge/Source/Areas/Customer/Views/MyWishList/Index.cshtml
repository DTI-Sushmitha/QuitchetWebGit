<script type="text/javascript">
    $(function () {
        $("#slider-range,#slider-range2,#slider-range3").slider({
            range: true,
            min: 0,
            max: 500,

            values: [75, 300],
            slide: function (event, ui) {
                $("#amount").val("$" + ui.values[0] + " - $" + ui.values[1]);
            }
        });
        $("#amount").val("$" + $("#slider-range,#slider-range2").slider("values", 0) +
          " - $" + $("#slider-range,#slider-range2,#slider-range3").slider("values", 1));
    });
    $('.slider').slider({
        handle: round,
        tooltip: true,
    });
