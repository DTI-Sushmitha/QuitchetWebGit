<script type="text/javascript">


    $(document).ready(function () {
        $('#selectimagepopup').hide();
        $('#googlemaps').hide();
        $('#removeentrypopup').hide();
        $('#addimagepopup').hide();
        $('#mynotespopup').hide();
        $('#savechangespopup').hide();
        $('#wishlistpopup').hide();
        $('#basicneedspopup').hide();
        $('#myphotospopup').hide();
        $('#uploadingpopup').hide();
        $(".nav-tabs a").click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

    });
    $('#browse1,#browse2').click(function () {
        if ($(this).attr("id") == "browse1") {
            $('#file1').click();
            $("#file1").change(function () {
                var t = document.getElementById("file1").files[0].name;
                $("#path1").val(t);
            });
        }
        if ($(this).attr("id") == "browse2") {
            $('#file2').click();
            $("#file2").change(function () {
                // alert($(":file").val());
                //$("#path2").val($("#file2").val());
                var t = document.getElementById("file2").files[0].name;
                $("#path2").val(t);
            });
        }
    });
    $("input[type='checkbox'][name='remove1']").change(function () {
        if ($("input[type='checkbox'][name='remove1']:checked").length) {
            $('#removeentrypopup').show();
        }
    })
    $("input[type='checkbox'][name='remove2']").change(function () {
        if ($("input[type='checkbox'][name='remove2']:checked").length) {
            $('#removeentrypopup').show();
        }
    })
    $("#mywishlistbtn").click(function () {
        $('#listingtyperow').show();
        $('#wishlistpopup').show();
        $('#basicneedspopup').hide();
        $('#myphotospopup').hide();
    });
    $("#addimagebtn").click(function () {
        $('#addimagepopup').show();

    });
    $("#myphotosbtn").click(function () {
        $('#mywishlistbtn').removeClass('active');
        $('#wishlistpopup').hide();
        $('#myphotospopup').hide();
        $('#basicneedspopup').hide();
        $('#listingtyperow').hide();

    });
    $("#mybasicneedbtn").click(function () {
        $('#mywishlistbtn').removeClass('active');
        $('#wishlistpopup').hide();
        $('#basicneedspopup').show();
        $('#myphotospopup').hide();
    });
    $("#noBtn").click(function () {

        $('#savechangespopup').hide();
    });
    $("#yesBtn").click(function () {

        $('#savechangespopup').hide();
    });
    $("#savechangesBtn").click(function () {

        $('#savechangespopup').show();
    });

    $("#savephotoBtn,#cancelphotoBtn").click(function () {
        $('#selectimagepopup').hide();
    });
    $("#addnotesbtn").click(function () {

        $('#mynotespopup').show();
    });
    $("#cancelnotesBtn,#donenotesBtn").click(function () {
        $('#mynotespopup').hide();
    });
    $("#removenoBtn,#removeyesBtn").click(function () {
        $('#remove1,#remove2').attr('checked', false);
        $('#removeentrypopup').hide();
    });
    $("#sharebtn,#sharebtn2").click(function () {

        $('#sharepopup').show();
    });
    $("#cancelsharebtn").click(function () {

        $('#sharepopup').hide();
    });
    $("#canceladdingphotoBtn").click(function () {

        $('#addimagepopup').hide();
    });
    $("#uploadphotoBtn").click(function () {
        var p1 = $('#path1').val();
        var p2 = $('#path2').val();
        // alert(p);
        $('#img1').text(p1);
        $('#img2').text(p2);
        $('#uploadingpopup').show();
        $('#addimagepopup').hide()

    });
    $("#canceluploadingBtn").click(function () {

        $('#uploadingpopup').hide();
    });

</script>
