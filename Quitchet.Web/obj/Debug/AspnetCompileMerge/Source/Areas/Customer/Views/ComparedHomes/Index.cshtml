    function up() {
        $('ul li').on('click', function (e) {
            var index = $(this).index();  // Index of clicked item
            var temp = $(this).html();    // Contents of clicked item
            var partner;            // The paired element
            partner = $(this).parent().find('li').get(index - 1);
            //spartner = $(this).parent().find('li').get(index + 1);
            // Put this in a try/catch to not throw errors for unmatched list items
            // (i.e. a list with 9 items, and then clicking on the 9th)

            if (index != 0) {
                try {
                    $(this).html(partner.innerHTML);
                    $(partner).html(temp);
                } catch (e) { }

            }
        });
    }


    function down() {
        $('ul li').on('click', function (e) {
            var index = $(this).index();  // Index of clicked item
            var temp = $(this).html();    // Contents of clicked item
            var partner;                  // The paired element

            partner = $(this).parent().find('li').get(index + 1);

            //spartner = $(this).parent().find('li').get(index + 1);
            // Put this in a try/catch to not throw errors for unmatched list items
            // (i.e. a list with 9 items, and then clicking on the 9th)
            if (index < 7) {
                try {
                    $(this).html(partner.innerHTML);
                    $(partner).html(temp);
                } catch (e) { }
            }

        });
    }

    $("#previousBtn").click(function (e) {
        $('#selectimagepopup').modal('show');
    });

    $("#backphotoBtn").click(function (e) {
        $("#fullphotopopup").hide();
    });
    $("#delphotoBtn").click(function (e) {
        $("#fullphotopopup").hide();
    });
    $("#img1,#img2,#img3").click(function (e) {
        //alert($(this).children('img').attr('src'));
        var p = $(this).children('img').attr('src');
        if (p == " ") {
            $('#selectimagepopup').show();
        }
        else {
            $('#fullimg').attr('src', p);
            $("#fullphotopopup").show();
        }
    });
    var flag_RightCollapse = 0;
    $('#Expand').on('click', function () {
        if (flag_RightCollapse == 0) {
            $("#Expand").attr('class', 'fa fa-angle-double-left');
            $("#CollDiv").animate({ 'left': '-3%' }, 'slow', function () {
            });
            $('#EditCompareViewsDiv').animate({ 'left': '25%' }, 'slow', function () {
                $('#EditCompareViewsDiv').hide();
            });
            flag_RightCollapse = 1;
        }
        else {
            $('#EditCompareViewsDiv').show();
            $("#CollDiv").animate({ 'left': '-26%' }, 'slow', function () {
            });
            $('#EditCompareViewsDiv').animate({ 'left': '1%' }, 'slow', function () {
            });
            $("#Expand").attr('class', 'fa fa-angle-double-right');
            flag_RightCollapse = 0;
        }
    });
    $("#savephotoBtn,#cancelphotoBtn").click(function () {
        $('#selectimagepopup').hide();
    });
</script>