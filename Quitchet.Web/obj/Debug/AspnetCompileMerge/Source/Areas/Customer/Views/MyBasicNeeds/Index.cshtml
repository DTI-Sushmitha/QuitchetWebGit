<script type="text/javascript">

    $(function () {
       
        $("#slider-range").slider({

            range: true,
            min: 0,
            max: 500,
            values: [0, 500],
            slide: function (event, ui) {
              

                var newPoint, offset, newPlace;

                var newPoint1, offset1, newPlace1;
                var minimum = 0;
                var maximum = 500;

                var el = $('#slider-range');
                var width = el.width();
               
                // Figure out placement percentage between left and right of input
                newPoint = (ui.values[0] - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
                newPoint1 = (ui.values[1] - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
                // Janky value to get pointer to line up better
                offset = -1.3;
                offset1 = -1.3;
                // Prevent bubble from going beyond left or right (unsupported browsers)
                if (newPoint < 0) { newPlace = 0; }
                else if (newPoint > 1) { newPlace = width; }
                else { newPlace = width * newPoint + offset; offset -= newPoint; }
               

                if (newPoint1 < 0) { newPlace1 = 0; }
                else if (newPoint1 > 1) { newPlace1 = width; }
                else { newPlace1 = width * newPoint1 + offset1; offset1 -= newPoint1; }
               
                //  el
                //.next("#stat")
                //.css({
                //    left: newPlace,
                //    marginLeft: offset + "%"
                //})
                //.text(el.val());

                $('#stat').css('left', newPlace);
                $('#stat').css('marginleft', offset+"%");
                
                $('#end').css('left', newPlace1-345);
                $('#end').css('marginleft', offset1 + "%");

                $('#stat').html(ui.values[0]);

                $('#end').html(ui.values[1]);
             }
        });
    });


    $(function () {

        $("#slider-range2").slider({

            range: true,
            min: 0,
            max: 500,
            values: [0, 500],
            slide: function (event, ui) {


                var newPoint, offset, newPlace;

                var newPoint1, offset1, newPlace1;
                var minimum = 0;
                var maximum = 500;

                var el = $('#slider-range2');
                var width = el.width();

                // Figure out placement percentage between left and right of input
                newPoint = (ui.values[0] - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
                newPoint1 = (ui.values[1] - parseInt(minimum)) / (parseInt(maximum) - parseInt(minimum));
                // Janky value to get pointer to line up better
                offset = -1.3;
                offset1 = -1.3;
                // Prevent bubble from going beyond left or right (unsupported browsers)
                if (newPoint < 0) { newPlace = 0; }
                else if (newPoint > 1) { newPlace = width; }
                else { newPlace = width * newPoint + offset; offset -= newPoint; }


                if (newPoint1 < 0) { newPlace1 = 0; }
                else if (newPoint1 > 1) { newPlace1 = width; }
                else { newPlace1 = width * newPoint1 + offset1; offset1 -= newPoint1; }

                //  el
                //.next("#stat")
                //.css({
                //    left: newPlace,
                //    marginLeft: offset + "%"
                //})
                //.text(el.val());

                $('#lstat').css('left', newPlace);
                $('#lstat').css('marginleft', offset + "%");

                $('#lend').css('left', newPlace1 - 345);
                $('#lend').css('marginleft', offset1 + "%");

                $('#lstat').html(ui.values[0]);

                $('#lend').html(ui.values[1]);
            }
        });
    });
   
 

</script>
