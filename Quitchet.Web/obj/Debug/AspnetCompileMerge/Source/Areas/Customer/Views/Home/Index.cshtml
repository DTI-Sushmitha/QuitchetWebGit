            $(this).tab('show');
        });

    });
    $("#searchsavedfilters").click(function (e) {
        $('#paneltoolbar1').hide();
        // e.preventDefault();
        $('#paneltoolbar2').show();

    });

    $("#searchfilters").click(function (e) {
        $('#middlemenu').hide();
        //    $('#paneltoolbar2').hide();
