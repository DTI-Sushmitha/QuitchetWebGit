        $('#popup1search').animate({ height: '820px' }, 500)
        $('#popup1search').show();
        $('#paneltoolbar2').hide();
    });


    $("#savedsearches").click(function (e) {
        $('#popup1search').hide();
        $('#middlemenu').hide();
        // e.preventDefault();
        $('#paneltoolbar1').hide();
        $('#paneltoolbar2').show();
