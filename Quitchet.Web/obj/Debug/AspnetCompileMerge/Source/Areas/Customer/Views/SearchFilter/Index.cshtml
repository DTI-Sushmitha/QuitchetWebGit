        if (value == 1) {
            DisplayName = 'Homes';
        }
        else if (value == 2) {
            DisplayName = 'Rentals';
            $(".collapse").collapse('hide');
        }
        else if (value == 3) {
            DisplayName = 'Lots/Land';
        }
        else {
            DisplayName = 'Property Type';
            $(".collapse").collapse('hide');
        }
        $('#PropSelected').html(DisplayName);
        if (value == 1) {
            $('#PropHomes').show();
            $('#PropLotsLands').hide();
        }
        else if (value == 3) {
            $('#PropLotsLands').show();
            $('#PropHomes').hide();
        }
        else {
            $('#PropHomes').hide();
            $('#PHSale,#PHConstruct,#PHSold').prop("checked", false);
        }
    });
