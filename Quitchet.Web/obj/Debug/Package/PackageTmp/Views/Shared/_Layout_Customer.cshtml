@*Layout for Entire Quitchet with Top Menu and Left Baeu Menu with their functionalities*@
@{
    Layout = "~/Views/Shared/CustomerListingViewLayout.cshtml";
    var UserFirstName = "";
    var UserLastName = "";
    var PkeyUserId = "0";
    var UserEmail = "";
    var UserPassword = "";
    var remember = "2";
    var UserType_ID = "0";
    var profilepath = "";
    var notsettingsurl = "";
    var myquitchetpath = "";
    var customersupportpath = "";
    var AgentIdVal = "0";
    var isManager = "";
    var UserProfilePic = "/img/addphotopic";
    if (Request.Cookies["UseridCookie"] != null)
    {
        customersupportpath = "/Customer/CustomerSupport/Index";
        if (Request.Cookies["UseridCookie"].Value != null)
        {
            PkeyUserId = Request.Cookies["UseridCookie"].Value;
            if (Request.Cookies["UserFirstNameCookie"].Value != "")
            {
                UserFirstName = Request.Cookies["UserFirstNameCookie"].Value.ToString();
            }
            else
            {
                UserFirstName = "No";
            }
            if (Request.Cookies["ProfilePicCookie"].Value != "")
            {
                UserProfilePic = Request.Cookies["ProfilePicCookie"].Value.ToString();
            }
            else
            {
                UserProfilePic = "/img/addphotopic.png";
            }
            if (Request.Cookies["UserTypeIDCookie"].Value != "")
            {
                UserType_ID = Request.Cookies["UserTypeIDCookie"].Value;
                if (UserType_ID == "2")
                {
                    notsettingsurl = "/Agent/AgentNotificationSettings/Index";
                    profilepath = "/Agent/AgentProfile/Index";
                    myquitchetpath = "/Agent/MyQuitchet/Index";
                    if (Request.Cookies["AgentIDCookie"].Value != null)
                    {
                        AgentIdVal = Request.Cookies["AgentIDCookie"].Value;
                    }
                    if (Request.Cookies["isManagerCookie"].Value != null)
                    {
                        isManager = Request.Cookies["isManagerCookie"].Value;
                    }
                }
                else if (UserType_ID == "1")
                {
                    notsettingsurl = "/Customer/NotificationSettings/Index";
                    myquitchetpath = "/Customer/MyQuitchet/Index";
                    profilepath = "/Customer/MyProfile/Index";
                }
            }
            else
            {
                UserType_ID = "";
            }
            if (Request.Cookies["UserLastNameCookie"].Value != "")
            {
                UserLastName = Request.Cookies["UserLastNameCookie"].Value.ToString();
            }
            else
            {
                UserLastName = "Name";
            }
        }
        else
        {
            PkeyUserId = "0";
        }
    }
    else
    {
        customersupportpath = "/GUSupportFeedback/Index";
    }
    if (Request.Cookies["RememberMyDeviceCookie"] != null)
    {
        if (Request.Cookies["RememberMyDeviceCookie"].Value == "1")
        {
            UserEmail = Request.Cookies["UserEmailCookie"].Value;
            UserPassword = Request.Cookies["UserPasswordCookie"].Value;
            remember = Request.Cookies["RememberMyDeviceCookie"].Value;
        }
    }
}
