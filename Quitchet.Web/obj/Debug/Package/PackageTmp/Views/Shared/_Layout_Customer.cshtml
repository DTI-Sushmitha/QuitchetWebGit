    //MEssage Sending Functionality
    (function () {
        Message = function (arg) {
            this.text = arg.text, this.message_side = arg.message_side;
            this.draw = function (_this) {
                return function () {
                    var $message;
                    $message = $($('.message_template').clone().html());
                    $message.addClass(_this.message_side).find('.text').html(_this.text);
                    $('.messages').append($message);
                    return setTimeout(function () {
                        return $message.addClass('appeared');
                    }, 0);
                };
            }(this);
            return this;
        };
        $(function () {
            //message_side = 'right';
            getMessageText = function () {

                $message_input = $('#message_input');
                return $message_input.val();
            };
            sendMessage = function (text,message_side,type) {
                $('#message_input').val('');
                var $messages, message;
                if (text.trim() === '') {
                    return;
                }
                var time = new Date();
                if(type=="send")
                {
                    m='';
                    //getChatHistory_Initial($("#chatid").text())
                    $('.messages .message.right .avatar').find("img").attr('src', '/img/addphotopic.png');
                    //$('#userpicinchat').attr('src', "data:image/png; base64," + myimageforchat);
                    @*var fname=@UserFirstName.ToString();
                            var lname=@UserLastName.ToString();
                            lname=fname+" "+lname;<div class="col-sm-6">'+lname+'</div>*@
                     //time = time.toLocaleString('en-US', { hour: 'number',minute:'number', hour12: true });
                      //m=text+'<br><div class="col-sm-12" class="col-sm-12" style="padding: 0px;  font-size: 9px; margin-top: 6px;margin-bottom:-12px;><div class="col-sm-6" style="padding-left: 0px;"><label>'+time+'</label></div></div>';
                }
                else
                {
                    //getChatHistory($("#chatid").text())
                    m=text;
                    $('#message_input').val('');
                    $messages = $('.messages');
                    message_side = message_side === 'left' ? 'right' : 'left';
                    message = new Message({
                        text: m,
                        message_side: message_side
                    });
                }
                if(type=="send"){
                    sendMsg(text);
                }
                if(type=="sendattach"){
                    sendMsg(filename);
                }

                filename="";
                message.draw();
                return $messages.animate({ scrollTop: $messages.prop('scrollHeight') }, 300);
            };
            $('.send_message').click(function (e) {
                if($('#chatid').text()=="")
                    return;
                else
                    return sendMessage(getMessageText(),'left','send');
            });
            $('#message_input').keyup(function (e) {
                if (e.which === 13) {
                    if($('#chatid').text()=="")
                        return;
                    else
                        return sendMessage(getMessageText(),'left','send');
                }
            });
        });
    }.call(this));

