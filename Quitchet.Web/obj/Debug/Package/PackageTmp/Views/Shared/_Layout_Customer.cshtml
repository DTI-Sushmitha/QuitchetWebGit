            document.getElementById("mySidebar").style.display = "none";
        }
        //Function to Sort Drop down in messages
        function mymsgDDClick() {
            document.getElementById("mymsgsortDropdown").classList.toggle("show");
            document.getElementById("agentmsgsortDropdown").classList.toggle("show");
        }
        function agentmsgDDClick() {
            document.getElementById("agentmsgsortDropdown").classList.toggle("show");
        }
        // Close the dropdown menu if the user clicks outside of it
        window.onclick = function(event) {
            if (!$(event.target).hasClass('msgdropbtn')) {
                var dropdowns = document.getElementsByClassName("msg-dropdown-content");
                var i;
                for (i = 0; i < dropdowns.length; i++) {
                    var openDropdown = dropdowns[i];
                    if (openDropdown.classList.contains('show')) {
                        openDropdown.classList.remove('show');
                    }
                }
            }
        }
        //---------Start: Escape Key Functionality-----
        $(document).keyup(function (e) {
            if (e.keyCode == 27) {
                // $("#formLogin").hide();
                $('#ProfileDropdown').hide();
            }
        });
