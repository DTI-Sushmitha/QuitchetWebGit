    ///PRP IMAGE SLIDER COMMON FUNCTION
    var imgindex = 0;
    function PrpImageSlider(index, i, divid, PrpImagesArray, mlsno) {
        var listofImages = []; var ii;
        listofImages = PrpImagesArray.split(',');
        if (i == "1") {
            imgindex = imgindex + 1;
            if (imgindex == listofImages.length)
                $("#Next" + mlsno).hide();
            else {
                if (imgindex == "0") {
                    ii = imgindex + 1
                    $("#" + divid).css("background-image", "url('" + listofImages[ii] + "')");
                    imgindex = ii;
                }
                else {
                    $("#" + divid).css("background-image", "url('" + listofImages[imgindex] + "')");
                }
                $("#Prev" + mlsno).show();
            }
        }
        else {
            imgindex = imgindex - 1
            imgindex = parseInt(imgindex);
            if (imgindex <= 0) {
                $("#Prev" + mlsno).hide();
            }
            else {
                $("#Prev" + mlsno).show();
            }
            $("#" + divid).css("background-image", "url('" + listofImages[imgindex] + "')");
            $("#Next" + mlsno).show();
        }
    }
</script>
