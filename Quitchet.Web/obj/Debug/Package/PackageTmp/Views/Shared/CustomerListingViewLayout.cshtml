    function HR_PopupSlideImage(ClickStatus, DIvisionID, PreviousButton, NextButton, ImageCountShow) {
        if (ClickStatus == 'Next') {
            CurrentImageCount = parseInt(CurrentImageCount + 1);
            $('#' + DIvisionID).css('background-image', 'url(' + Arr_PropertyPhotos[CurrentImageCount - 1] + ')');
            $('#' + PreviousButton).show();
            if (parseInt(CurrentImageCount) == parseInt(CurrentPhotosCount))
                $('#' + NextButton).hide();
        }
        if (ClickStatus == 'Previous') {
            CurrentImageCount = CurrentImageCount - 1;
            if (parseInt(CurrentImageCount) == 1)
                $('#' + PreviousButton).hide();
            $('#' + NextButton).show();
            $('#' + DIvisionID).css('background-image', 'url(' + Arr_PropertyPhotos[CurrentImageCount - 1] + ')');
        }
        $('#' + ImageCountShow).html(CurrentImageCount);
    }
