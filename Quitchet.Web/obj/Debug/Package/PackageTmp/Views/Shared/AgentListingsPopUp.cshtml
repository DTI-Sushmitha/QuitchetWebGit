<style>
    .RoundEdgespn {
        display: inline-block;
        min-width: 10px;
        padding: 3px 7px;
        font-size: 12px;
        font-weight: 700;
        line-height: 1;
        color: #fff;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        background-color: #777;
        border-radius: 4px;
    }

    .btnInactiveClass {
        border: none !important;
        color: black !important;
        width: 100%;
        background-color: #ffffff !important;
        font-size: 14px;
        left: 10px;
    }

    .btnActiveClass {
        border: none !important;
        color: white !important;
        width: 100%;
        background-color: #f56331 !important;
        font-size: 14px;
    }

    .firstLabel {
        font-weight: bold;
    }

    .SecondLabelRed {
        font-weight: bold;
        color: #f56331;
    }

    .EminitiesSecondLabelRed {
        font-weight: bold;
        color: #f56331;
    }

    .listPointer {
        font-weight: bold;
        font-size: 25px;
        color: #f56331;
        position: relative;
        left: -6px;
    }

    .textlimit {
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    .removetextlimit {
        overflow: hidden;
        white-space: normal;
    }

    .AgentPopUpModel {
        background-color: #f56331;
        color: white;
        text-align: center;
        padding-top: 5px;
        padding-bottom: 5px;
        padding-left: 0px;
        padding-right: 0px;
        position: relative;
        margin-top: -0.5%;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
    }

    .MainToggleDivision {
        position: relative;
        width: 100.6%;
        background-color: #ffffff;
    }

    #div_inPOPup::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
        border-radius: 20px;
        background-color: #ffffff;
