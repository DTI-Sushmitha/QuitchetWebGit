<script>
    //RAting Functionalty in Feedback Popup
    var count = "0";
    function openFeedbackPopup() {
        $('#Post_Feedback_Popup').modal('show')
        $("#PFB_MLS_Image").attr('src', $('#hidden_propertyimage').text());
        $("#PFB_AddressLineOne").text($('#hidden_addr1').text())
        $("#PFB_AddressLineTwo").text($('#hidden_addr2').text())
        $("#PFB_MLSID").text('MLS#' + $('#hidden_mlsid').text());
