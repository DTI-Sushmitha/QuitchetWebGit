    function readUploadFiles(input) {
        var length = input.files.length;
        for (var i = 0; i < length; i++) {
            if (input.files && input.files[i]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    Arr_MultiUploadImages.push((e.target.result).split(',')[1]);
                    var BaseString = ((e.target.result).split(',')[1]);
                    $.ajax({
                        url: "/FullDetails/PostAGLMultiUploadImages/",
                        type: "POST",
                        dataType: "json",
                        data: { AgentID: AgentID, MLSID: MlsID, PhotoBase: BaseString },
                        success: function (data) {
                            GetFullDetails(AgentID, MlsID);
                        },
                        error: function () {
                        }
                    });
                }
                reader.readAsDataURL(input.files[i]);
            }
            var file = input.files[i];
        }
    }
