    function AGL_PopupSlideImage(ClickStatus, DIvisionID, PreviousButton, NextButton, ImageCountShow) {
        if (ClickStatus == 'Next') {
            ImageCount = parseInt(ImageCount + 1);
            $('#' + DIvisionID).css('background-image', 'url(' + Arr_Images[ImageCount - 1] + ')');
            $('#' + PreviousButton).show();
            if (parseInt(ImageCount) == parseInt(PhotosCount))
                $('#' + NextButton).hide();
        }
        if (ClickStatus == 'Previous') {
            ImageCount = ImageCount - 1;
            if (parseInt(ImageCount) == 1)
                $('#' + PreviousButton).hide();
            $('#' + NextButton).show();
            $('#' + DIvisionID).css('background-image', 'url(' + Arr_Images[ImageCount - 1] + ')');
        }
        $('#' + ImageCountShow).html(ImageCount);
    }

