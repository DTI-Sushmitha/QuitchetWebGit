<script type="text/javascript">
    var MlsID = ''; var AgentID = ''; var AGL_SharePopURL = ""; var HomeType = ''; var Arr_Images = []; var ImageCount = 1; var PhotosCount = 0; var Str_Geolat = ''; var Str_Geolang = ''; var Arr_Showings = []; var Arr_DocOnFiles = ''; var Arr_DocWithFiles = ''; var Arr_MultiUploadImages = []; var Favourite = '';
    var Arr_lakeProperties = []; var Arr_Equestrian = []; var Arr_RoadFrontage = []; var Arr_Topography = []; var Arr_Aminities = []; var Arr_PropertyDescription = [];
    $(document).ready(function () {
        var Url = new URLSearchParams(window.location.search);
        MlsID = Url.get("MLSID");
        AgentID = Url.get("AGENTID");
        AgentFullDetailsView(AgentID, MlsID);
    })
    var MlsID = ''; var AgentID = ''; var AGL_SharePopURL = ""; var HomeType = ''; var Arr_Images = []; var ImageCount = 1; var PhotosCount = 0; var Str_Geolat = ''; var Str_Geolang = ''; var Arr_Showings = []; var Arr_DocOnFiles = ''; var Arr_DocWithFiles = ''; var Arr_MultiUploadImages = [];
    function AgentFullDetailsView(RefAgentID, RefMlsID) {
        AgentID = RefAgentID;
        MlsID = RefMlsID;
        GetFullDetails(RefAgentID, RefMlsID);
    }
    function GetFullDetails(AgentID, MlsID) {
        $.ajax({
            url: "/FullDetails/GetFullDetailsByMLSIFD",
            type: "GET",
            dataType: "json",
            data: { AgentID: AgentID, MlsID: MlsID },
            success: function (data) {
                if (data[0].Class == "Residential" || data[0].Class == "Rental") {
                    BindDatatoPopUp(data);
                    $('#div_homeRentals_MlsDiv').show();
                    $('#div_LotsLand_MlsDiv').hide();
                }
                if (data[0].Class == "Lots/Land") {
                    BindLotsLandsDatatoPopUp(data);
                    $('#div_homeRentals_MlsDiv').hide();
                    $('#div_LotsLand_MlsDiv').show();
                }
            },
            error: function () {
