                if(AddedTimeStatus=="True")
                {
                    counter=CheckOutDuration*60;
                }
                else{
                    CallEmergencyService();
                    $('#Expries_Countdown_popup').modal('hide');
                    $('#btnAddTime').hide();
                    $('#CheckOutDiv').hide();
                }
            }
        }, 1000);

