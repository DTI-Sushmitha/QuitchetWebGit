    function updateClock() {
        var timer2 = CheckOutDuration;
        if (timer2 > 59) {
            var Thours = Math.floor(parseInt(timer2) / 60);
            var Tminutes = parseInt(timer2) % 60;
        }
        else {
            var Thours = 00;
            var Tminutes = timer2;
        }

        var counter = CheckOutDuration * 60;
        var interval = setInterval(function () {
            counter--;
            // Display 'counter' wherever you want to display it.
            if (counter <= 60 && counter > 0) {
                // Display a login box
                if ($('#Expries_Countdown_popup').is(':visible') == true) {

                }
                else {
                    $('#Expries_Countdown_popup').modal('show');
                }
            }
            if (counter == 0) {
                if (AddedTimeStatus == "True") {
                    counter = CheckOutDuration * 60;
                }
                else {
                    CallEmergencyService();
                    $('#Expries_Countdown_popup').modal('hide');
                    $('#btnAddTime').hide();
                    $('#CheckOutDiv').hide();
                }
            }
        }, 1000);
        $(function () {
            var countdowntimerlimit = $("#CountDownSpan" + Pkey_PublicOpenHouseID + count).countdowntimer({
                hours: Thours,
                minutes: Tminutes,
                seconds: 0,
                timeUp: timeisUp,
                //stopButton : "DestroyID"
            });
            function timeisUp() {
            }
        });
        $(function () {
            var popupcountdowntimer = $("#PopupCountDownSpan" + Pkey_PublicOpenHouseID + count).countdowntimer({
                hours: Thours,
                minutes: Tminutes,
                seconds: 0,
                timeUp: timeisUp,
            });
            function timeisUp() {
                // $('#btnCheckOut').click();
                // $('#btnAddTime').hide();
            }
        });
    }
