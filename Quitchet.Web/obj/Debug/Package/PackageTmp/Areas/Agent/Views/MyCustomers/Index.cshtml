    function ConvertToDate(ReqDate) {
        var M = ReqDate.split(',')[1];
        var m = M.split('.')[0];
        switch (m) {
            case "Jan": m = 1; break;
            case "Feb": m = 2; break;
            case "Mar": m = 3; break;
            case "Apr": m = 4; break;
            case "May": m = 5; break;
            case "Jun": m = 6; break;
            case "Jul": m = 7; break;
            case "Aug": m = 8; break;
            case "Sep": m = 9; break;
            case "Oct": m = 10; break;
            case "Nov": m = 11; break;
            case "Dec": m = 12; break;
            default: m = 1; break;
        }

        var rem = M.split('.')[1];
        var d = rem.split(',')[0];
        var y = ReqDate.split(',')[2];
        var converteddate = m + '/' + d + '/' + y;
        return converteddate;
    }
