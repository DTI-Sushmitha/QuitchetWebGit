<script>
    var listtype = "1";
    $(".dropdown-content a").click(function () {
        $("#typename").text($(this).text());
        $("#propertytype > option").attr("selected", false);
        $('#propertytype').multiselect('refresh');
        $("#lotspropertytype > option").attr("selected", false);
        $('#lotspropertytype').multiselect('refresh');
        propertytext = "Any";
        hproperty = "Any";
        if ($(this).attr('value') == "1") {
            listtype = "1";
            $('#DivLotAcreage').hide();
            $('#DivBedBathRooms').show();
            $('#DivMasterBedroom').show();
            $('#rentalslider-range,#rental_formatted_from_price,#rental_formatted_to_price').hide();
            $('#slider-range,#forsale_formatted_from_price,#forsale_formatted_to_price').show();
            $('#lotspropertytypediv').hide();
            $('#propertytypediv').show();
            getdetails(1);

        }
        else if ($(this).attr('value') == "2") {
            listtype = "2";
            $('#DivLotAcreage').hide();
            $('#DivBedBathRooms').show();
            $('#DivMasterBedroom').hide();
            $('#rentalslider-range,#rental_formatted_from_price,#rental_formatted_to_price').show();
            $('#slider-range,#forsale_formatted_from_price,#forsale_formatted_to_price').hide();
            $('#lotspropertytypediv').hide();
            $('#propertytypediv').show();
            getdetails(2);
        }
        else if ($(this).attr('value') == "3") {
            listtype = "3";
            $('#DivLotAcreage').show();
            $('#DivBedBathRooms').hide();
            $('#DivMasterBedroom').hide();
            $('#rentalslider-range,#rental_formatted_from_price,#rental_formatted_to_price').hide();
            $('#slider-range,#forsale_formatted_from_price,#forsale_formatted_to_price').show();
            $('#lotspropertytypediv').show();
            $('#propertytypediv').hide();
            getdetails(3);
        }
    })

</script>
