            if(imageIndex-1!=0)
            {
                $('#HomepopupPrevImgBtn').show();
            }
            $('#HomePopupImgDiv').css('background-image','url('+NxImg +')');
        }
        else{
            var c= imageIndex;
            $('#HomePresentPhotoNum').html(c);
            if(imageIndex-1==0)
            {
                $('#HomepopupPrevImgBtn').hide();
            }
            if(imageIndex+1!=(PopupPhotos.length-1))
            {
                $('#HomepopupNextImgBtn').show();
            }
            $('#HomePopupImgDiv').css('background-image','url('+PvImg +')');
        }
    }

