<style>
    #ListingsView_Div::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
        border-radius: 20px;
        background-color: #ffffff;
    }

    #ListingsView_Div::-webkit-scrollbar {
        width: 0.8%;
        background-color: #ff5d26;
    }

    #ListingsView_Div::-webkit-scrollbar-thumb {
        border-radius: 20px;
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
        background-color: #ff5d26;
    }

    #borderdiv:after {
        content: "";
        position: absolute;
        right: 0;
        z-index: 100;
        top: 10px;
        width: 1px;
        height: 80%; /* or 100px */
        background: #f37d55;
    }

    #pointeddiv:before {
        position: absolute;
        top: 40%;
        left: 100%;
        content: '';
        width: 0;
        height: 0;
        margin: -5px 0 0;
        border-left: solid 15px rgba(254, 209, 197, 1);
        border-bottom: solid 15px transparent;
        border-top: solid 15px transparent;
    }

    #a_listings:after {
        content: "";
        position: absolute;
        right: 0;
        z-index: 100;
        top: 10px;
        width: 1px;
        height: 80%; /* or 100px */
        background: #f37d55;
    }

    #div_inPOPup::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
        border-radius: 20px;
        background-color: #ffffff;
    }

    #div_inPOPup::-webkit-scrollbar {
        width: 2%;
        background-color: #ef4308b5;
        background: #fff;
    }

    #div_inPOPup::-webkit-scrollbar-thumb {
        border-radius: 20px;
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
        background-color: #ff5d26;
    }
    /*Common Scroll Class*/
    .scrolls_divs::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
        border-radius: 10px;
        background-color: #ffffff;
