                            if (lsp.indexOf(',') != -1) {
                                var dataarray = lsp.split(",");
                                $("#lpropertytype").multiselect("select", dataarray);
                            }
                            else {
                                $("#lpropertytype").multiselect("select", lsp);
                            }
                        }
                        var llot = data[0].LotFeatures;
                        llotfeaturestext = llot;
                        if (llot == "Any") {
                            $("#llotfeatures").multiselect('selectAll', false);
                            //$("#lotslotfeatures").multiselect('updateButtonText');
                        }
                        else {
                            if (llot.indexOf(',') != -1) {
                                var dataarray = llot.split(",");
                                $("#llotfeatures").multiselect("select", dataarray);
                            }
                            else {
                                $("#llotfeatures").multiselect("select", llot);
                            }
                        }
                        LotSummerNew();
                    }
                }
                else {
                    //resetOnlySliders()
                }
            },
            error: function () {
                resetOnlySliders()
            }
        });
