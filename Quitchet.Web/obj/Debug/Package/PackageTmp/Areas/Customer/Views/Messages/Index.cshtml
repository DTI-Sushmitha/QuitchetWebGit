    //function which is executed when zoom changed
    function zoomchanged(zoom) {
        if(poly=="")
        {
            if (zoom <= 8) {
                $('#drawpoly').css("visibility", "hidden");
                for (var i = 0; i < m.length; i++) {
                    m[i].setMap(null);
                }
            }
            else {

                $('#drawpoly').css("visibility", "visible");
                for (var i = 0; i < m.length; i++) {
                    m[i].setMap(map);
                }
            }
        }
        else
        {
            if (zoom <= 8) {
                $('#drawpoly').css("visibility", "hidden");
                for (var i = 0; i < m.length; i++) {
                    m[i].setMap(null);
                }
                poly.setMap(null)
            }
            else {
