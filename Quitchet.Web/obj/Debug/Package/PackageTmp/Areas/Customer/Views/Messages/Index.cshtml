    <div class="modal-dialog" role="document" style="padding-top: 15%; width: 25%">
        <div class="modal-content">
            <div class="col-sm-12" style="margin-top: 3%">
                <label for="inputEmail3" class="control-label" style="width: 100%; color: #FF6937 !important;">Note</label>
            </div>
            <div class="col-sm-12">
                @Html.TextArea("MyNote", "", new { @class = "form-control ", @placeholder = "Please Enter", style = "height:120px!important;border: 1px solid #f56331;background-color: rgba(245, 99, 49, 0.12);" })
                <div id="note"><span id="span_mynote" style="color: red; font-size: 10px"></span></div>
            </div>
            <div class="col-sm-12">
                <hr style="border: 0 ! important; margin-top: 2%; margin-bottom: 5%; border-top: 1px solid  #FF6937 !important;" />
            </div>
            <div class="modal-footer">
                <div class="col-sm-6" style="text-align: left; padding-left: 0px">
                    <a href="#" class="btn" id="Mynote_Cancel" style="border-radius: 5px !important; background-color: gray; color: white; font-weight: 500; font-size: 11px;">Cancel</a>
                </div>
                <div class="col-sm-6" style="text-align: right; padding-right: 0px">
                    <a href="#" class="btn" id="Mynote_Save" style="border-radius: 5px !important; background-color: #FF6937 !important; color: white; font-weight: 500; font-size: 11px;">Done</a>
                </div>
            </div>
        </div>
    </div>
</div>
@* END User My Note PopUp*@
